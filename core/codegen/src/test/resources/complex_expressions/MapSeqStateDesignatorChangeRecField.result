import org.overture.codegen.runtime.*;

import java.util.*;


public class Entry {
    private VDMMap m = MapUtil.map(new Maplet(true, new Rec(10L)));

    public Entry() {
    }

    public void modify() {
        ((Rec) m.get(true)).field = 20L;
    }

    public Number read() {
        return ((Rec) m.get(true)).field;
    }

    public static Object Run() {
        Entry a = new Entry();
        a.modify();

        return a.read();
    }

    public String toString() {
        return "Entry{" + "m := " + Utils.toString(m) + "}";
    }

    private static class Rec implements Record {
        public Number field;

        public Rec(final Number _field) {
            field = _field;
        }

        public boolean equals(final Object obj) {
            if (!(obj instanceof Rec)) {
                return false;
            }

            Rec other = ((Rec) obj);

            return field.longValue() == other.field.longValue();
        }

        public int hashCode() {
            return Utils.hashCode(field);
        }

        public Rec clone() {
            return new Rec(field);
        }

        public String toString() {
            return Utils.recordToString(this, field);
        }
    }
}

