import org.overture.codegen.runtime.*;

import java.util.*;


public class A {
    public A() {
    }

    public Number op1(A a) {
        return a.getMe().f();
    }

    public Number op2(A a) {
        return a.getMe().getMe().f();
    }

    public Number op3(A a) {
        return a.getMe().getMe().getMe().f();
    }

    public A getMe() {
        return this;
    }

    public static Number f() {
        return 1L;
    }
}

