import org.overture.codegen.runtime.*;

import java.util.*;


public class Entry {
    public Entry() {
    }

    public static Object Run() {
        A a = new A();

        return a.op1(a).longValue() + a.op2(a).longValue() +
        a.op3(a).longValue();
    }
}

