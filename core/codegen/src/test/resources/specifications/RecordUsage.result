import org.overture.codegen.runtime.*;

import java.util.*;


public class A {
    private static final R2 a = new R2(new R1(2L), 3L);
    private static final R2 b = Utils.clone(a);
    private static final R1 c = Utils.clone(a.r1);
    private static final Number d = a.r1.x;

    public A() {
    }

    public Number op1() {
        R1 r = new R1(2L);
        r.x = 3L;

        return r.x;
    }

    public Number op2() {
        R1 a = new R1(5L);
        R1 b = Utils.clone(a);

        return b.x;
    }

    public Number op3() {
        R2 a = new R2(new R1(2L), 3L);
        R1 b = Utils.clone(a.r1);
        a.x = 1L;
        a.r1.x = 2L;

        return a.r1.x;
    }

    public R2 op4() {
        R2 a = new R2(new R1(2L), 3L);

        return Utils.clone(a);
    }

    public R1 op5() {
        R2 a = new R2(new R1(2L), 3L);

        return Utils.clone(a.r1);
    }

    public String toString() {
        return "A{" + "a = " + Utils.toString(a) + ", b = " +
        Utils.toString(b) + ", c = " + Utils.toString(c) + ", d = " +
        Utils.toString(d) + "}";
    }

    public static class R1 implements Record {
        public Number x;

        public R1(final Number _x) {
            x = _x;
        }

        public boolean equals(final Object obj) {
            if (!(obj instanceof R1)) {
                return false;
            }

            R1 other = ((R1) obj);

            return Utils.equals(x, other.x);
        }

        public int hashCode() {
            return Utils.hashCode(x);
        }

        public R1 clone() {
            return new R1(x);
        }

        public String toString() {
            return "mk_A`R1" + Utils.formatFields(x);
        }
    }

    public static class R2 implements Record {
        public R1 r1;
        public Number x;

        public R2(final R1 _r1, final Number _x) {
            r1 = (_r1 != null) ? Utils.clone(_r1) : null;
            x = _x;
        }

        public boolean equals(final Object obj) {
            if (!(obj instanceof R2)) {
                return false;
            }

            R2 other = ((R2) obj);

            return (Utils.equals(r1, other.r1)) && (Utils.equals(x, other.x));
        }

        public int hashCode() {
            return Utils.hashCode(r1, x);
        }

        public R2 clone() {
            return new R2(r1, x);
        }

        public String toString() {
            return "mk_A`R2" + Utils.formatFields(r1, x);
        }
    }
}

