        schedule = (exs.size() == 1L)
            ? MapUtil.domResBy(SetUtil.set(p), Utils.clone(schedule))
            : MapUtil.override(Utils.clone(schedule),
                MapUtil.map(
                    new Maplet(p,
                        SetUtil.diff(Utils.clone(exs), SetUtil.set(ex)))));
